PORT=3000
NODE_ENV=development
LOG_LEVEL=info
USE_PROXY=false

CORS_ALLOWLIST=http://localhost:5173,http://localhost:3000

# Docker service name for Postgres matches compose
DATABASE_URL=postgresql://XXXX:XXXX@db:XXXX/routing?schema=public

# External services
PITEAS_API_BASE_URL=https://sdk.piteas.io
RPC_URL=https://rpc.pulsechain.com

# pulsex routers (fallback)
PULSEX_V1_ROUTER=0x1715a3E4A142d8b698131108995174F37aEBA10D
PULSEX_V2_ROUTER=0x165C3410fC91EF562C50559f7d2289fEbed552d9
PULSEX_STABLE_ROUTER=0xDA9aBA4eACF54E0273f56dfFee6B8F1e20B23Bba

# connectors (fallback tries direct and through WPLS/USDC/USDT/DAI)
WPLS=0xA1077a294dDE1B09bB078844df40758a5D0f9a27
USDC=0x15D38573d2feeb82e7ad5187aB8c1D52810B1f07 # “USDC from Ethereum”
USDT=0x0Cb6F5a34ad42ec934882A05265A7d5F59b51A2f # “USDT from Ethereum”
DAI=0xefD766cCb38EaF1dfd701853BFCe31359239F305 # “DAI from Ethereum”

# allowlist policy
ENFORCE_ALLOWED_DEXES=true
ALLOWED_DEXES=pulsexv1,pulsexv2,pulsexstable,phux,9inchv2,9inchv3,9mmv2,9mmv3,pdexv3,dextop,tide

FALLBACK_ENABLE_THREE_HOP=false
FALLBACK_DEBUG = "1"
FALLBACK_SPLIT_STEP_BPS=250

# Optional keys
CHANGENOW_API_KEY=dev_placeholder
# PROXY_LIST=
# PROXY_USERNAME=
# PROXY_PASSWORD=

# Rate limits
RL_POINTS=200
RL_DURATION=60
QUOTE_RL_POINTS=10
QUOTE_RL_DURATION=5

# Onramps providers JSON path
ONRAMPS_JSON_PATH=./data/onramps_providers.json

# MoonPay
MOONPAY_PUBLISHABLE_KEY=XXXX
MOONPAY_SECRET_KEY=XXXX

# Transak (publishable)
TRANSAK_KEY=XXXX
TRANSAK_REFERRER_DOMAIN=localhost

# Ramp (publishable)
RAMP_HOST_API_KEY=XXXX
RAMP_HOST_APP_NAME=XXXX
RAMP_FINAL_URL=https://localhost:5173/bridge