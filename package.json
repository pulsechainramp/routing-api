{
  "name": "routing-api",
  "version": "1.0.0",
  "description": "API Gateway for Piteas with proxy rotation and rate limiting",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "prisma generate",
    "build": "tsc && npm run copy-generated",
    "copy-generated": "node scripts/copy-generated.js",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "stress:pulsex": "tsc --project tsconfig.scripts.json && node dist/scripts/stress/pulsexQuote.stress.js",
    "pulsex:test-router-math": "ts-node scripts/pulsex/compareRouterMath.ts",
    "pulsex:compare-router": "ts-node scripts/pulsex/compareRouterMath.ts",
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/helmet": "13.0.1",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "@fastify/jwt": "9.1.0",
    "@prisma/client": "5.22.0",
    "axios": "1.13.1",
    "bottleneck": "2.19.5",
    "dotenv": "16.5.0",
    "ethers": "6.14.3",
    "fastify": "5.4.0",
    "geoip-lite": "1.4.10",
    "https-proxy-agent": "7.0.6",
    "pino": "9.14.0",
    "pino-pretty": "10.3.1",
    "rate-limiter-flexible": "2.4.2",
    "siwe": "2.3.2",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@types/geoip-lite": "1.4.4",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.7",
    "autocannon": "^7.14.0",
    "jest": "29.7.0",
    "prisma": "5.22.0",
    "ts-jest": "29.3.4",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3"
  }
}
